     </ListGroup>\r\n          </Col>\r\n          <Col md={3}>\r\n            <Card>\r\n              <ListGroup variant=\"flush\">\r\n                <ListGroup.Item>\r\n                  <Row>\r\n                    <Col>Price:</Col>\r\n                    <Col>\r\n                      <strong>${product.price}</strong>\r\n                    </Col>\r\n                  </Row>\r\n                </ListGroup.Item>\r\n                <ListGroup.Item>\r\n                  <Row>\r\n                    <Col>Stock:</Col>\r\n                    <Col>\r\n                      <strong>\r\n                        {product.countInStock > 0 ? \"In Stock\" : \"Out Of Stock\"}\r\n                      </strong>\r\n                    </Col>\r\n                  </Row>\r\n                </ListGroup.Item>\r\n                {product.countInStock > 0 && (\r\n                  <ListGroup.Item>\r\n                    <Row>\r\n                      <Col>Qty</Col>\r\n                      <Col>\r\n                        <Form.Control\r\n                          as=\"select\"\r\n                          value={qty}\r\n                          onChange={(e) => setQty(e.target.value)}\r\n                        >\r\n                          {[...Array(product.countInStock).keys()].map((x) => (\r\n                            <option key={x + 1} value={x + 1}>\r\n                              {x + 1}\r\n                            </option>\r\n                          ))}\r\n                        </Form.Control>\r\n                      </Col>\r\n                    </Row>\r\n                  </ListGroup.Item>\r\n                )}\r\n                <ListGroup.Item>\r\n                  <Button\r\n                    className=\"btn-block\"\r\n                    type=\"button\"\r\n                    onClick={addToCartHandler}\r\n                    disabled={product.countInStock === 0}\r\n                  >\r\n                    Add To Cart\r\n                  </Button>\r\n                </ListGroup.Item>\r\n              </ListGroup>\r\n            </Card>\r\n          </Col>\r\n        </Row>\r\n      )}\r\n    </>\r\n  );\r\n}\r\n\r\nProductScreen.propTypes = {};\r\n\r\nexport default ProductScreen;\r\n","C:\\Users\\Watashi-Pasacon\\PycharmProjects\\phase 4\\ProShop\\frontend\\src\\store.js",[],"C:\\Users\\Watashi-Pasacon\\PycharmProjects\\phase 4\\ProShop\\frontend\\src\\reducers\\productReducers.js",[],["107","108"],"C:\\Users\\Watashi-Pasacon\\PycharmProjects\\phase 4\\ProShop\\frontend\\src\\constants\\productConstants.js",[],"C:\\Users\\Watashi-Pasacon\\PycharmProjects\\phase 4\\ProShop\\frontend\\src\\components\\Loader.js",[],["109","110"],"C:\\Users\\Watashi-Pasacon\\PycharmProjects\\phase 4\\ProShop\\frontend\\src\\actions\\productActions.js",[],"C:\\Users\\Watashi-Pasacon\\PycharmProjects\\phase 4\\ProShop\\frontend\\src\\components\\Message.js",[],"C:\\Users\\Watashi-Pasacon\\PycharmProjects\\phase 4\\ProShop\\frontend\\src\\reducers\\cartReducers.js",[],"C:\\Users\\Watashi-Pasacon\\PycharmProjects\\phase 4\\ProShop\\frontend\\src\\Screens\\CartScreen.js",[],"C:\\Users\\Watashi-Pasacon\\PycharmProjects\\phase 4\\ProShop\\frontend\\src\\actions\\cartActions.js",[],"C:\\Users\\Watashi-Pasacon\\PycharmProjects\\phase 4\\ProShop\\frontend\\src\\constants\\cartConstants.js",[],"C:\\Users\\Watashi-Pasacon\\PycharmProjects\\phase 4\\ProShop\\frontend\\src\\components\\FromContainer.js",[],"C:\\Users\\Watashi-Pasacon\\PycharmProjects\\phase 4\\ProShop\\frontend\\src\\Screens\\LoginScreen.js",[],"C:\\Users\\Watashi-Pasacon\\PycharmProjects\\phase 4\\ProShop\\frontend\\src\\actions\\userActions.js",[],{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","replacedBy":"114"},{"ruleId":"111","replacedBy":"115"},{"ruleId":"113","replacedBy":"116"},{"ruleId":"111","replacedBy":"117"},{"ruleId":"113","replacedBy":"118"},{"ruleId":"119","severity":1,"message":"120","line":16,"column":8,"nodeType":"121","messageId":"122","endLine":16,"en